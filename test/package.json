    "@types/google.maps": "^3.45.4",
    "@types/node": "^15.12.5",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/composition-api": "^1.0.0-rc.12",
    "core-js": "^3.6.5",
    "cpx": "^1.5.0",
    "rxjs": "^6.6.7",
    "typescript": "~4.1.5",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
