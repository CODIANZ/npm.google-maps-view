{
  "name": "google-maps-view-test",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode local",
    "copy-dist": "cpx -C ../dist/**       ./node_modules/@codianz/google-maps-view/dist",
